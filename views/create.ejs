    <div class="container mt-4">
        <div class="card border-secondary">
            <div class="card-header">NUEVO REGISTRO</div>
            <div class="card-body">
                <form action="/save" method="POST">
                    <div class="mb-3">
                        <label for="user" class="form-label">ID</label>
                        <input type="text" class="form-control" id="id" name="id" tabindex="1">
                    </div>
                    <div class="mb-3">
                        <label for="user" class="form-label">NOMBRES</label>
                        <input type="text" class="form-control" id="nombres" name="nombres" tabindex="2">
                    </div>
                    <div class="mb-3">
                        <label for="user" class="form-label">APELLIDOS</label>
                        <input type="text" class="form-control" id="apellidos" name="apellidos" tabindex="3">
                    </div>
                    <div class="mb-3">
                        <label for="user" class="form-label">DIRECCION</label>
                        <input type="text" class="form-control" id="direccion" name="direccion" tabindex="4">
                    </div>
                    <div class="mb-3">
                        <label for="user" class="form-label">E-MAIL</label>
                        <input type="text" class="form-control" id="email" name="email" tabindex="5">
                    </div>
                    <div class="mb-3">
                        <label for="user" class="form-label">CELULAR</label>
                        <input type="text" class="form-control" id="celular" name="celular" tabindex="6">
                    </div>
                    <div class="mb-3">
                        <label for="user" class="form-label">CIUDAD</label>
                        <select id="ciudad" name="ciudad" class="form-select" aria-label="Default select example"
                            tabindex="5">
                            <option value="MONTERIA" >MONTERIA</option>
                            <option value="CERETE">CERETE</option>
                            <option value="MEDELLIN" >MEDELLIN</option>
                            <option value="CALI">CALI</option>
                            <option value="BOGOTA" >BOGOTA</option>                                            
                        </select>
                    </div>
                    <div class="mb-3">
                        <label for="user" class="form-label">DEPARTAMENTO</label>
                        <select id="departamento" name="departamento" class="form-select" aria-label="Default select example"
                            tabindex="5">
                            <option value="CORDOBA" >CORDOBA</option>
                            <option value="ANTIOQUIA">ANTIOQUIA</option>
                            <option value="CAUCA" >CAUCA</option>
                            <option value="CUNDINAMARCA">CUNDINAMARCA</option>
                            <option value="NARIÑO" >NARIÑO</option>                                            
                        </select>
                    </div>
                    <button type="submit" class="btn btn-primary" tabindex="5">Guardar</button>
                    <a href="/index2" class="btn btn-secondary">Cancelar</a>
                </form>
            </div>
        </div>
    </div>
    </div>

